{
    "name": "kz370/krayin-multi-tenancy",
    "description": "A multi-tenancy package for Krayin CRM.",
    "keywords": ["laravel", "krayin", "crm", "multi-tenancy"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\MultiTenancy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\MultiTenancy\\Providers\\MultiTenancyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
